<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="App\Repository\UserRepository" name="App\Entity\User" table="symfony_demo_user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fullName" type="string" column="full_name" nullable="false"/>
    <field name="username" type="string" column="username" unique="true" nullable="false"/>
    <field name="email" type="string" column="email" unique="true" nullable="false"/>
    <field name="password" type="string" column="password" nullable="false"/>
    <field name="roles" type="json" column="roles" nullable="false"/>
  </entity>
</doctrine-mapping>
